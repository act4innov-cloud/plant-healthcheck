{
  "templates": [
    {
      "id": "TPL_COMP_001",
      "equipmentType": "compression",
      "title": "Inspection Compresseur - Norme ISO 45001 OCP",
      "description": "Checklist standard pour l'inspection des compresseurs industriels selon les normes OCP et ISO 45001",
      "version": "2.1",
      "frequency": "monthly",
      "estimatedDuration": 45,
      "requiredCertifications": ["ISO 45001", "Habilitation électrique"],
      "requiredPPE": [
        "Casque de sécurité",
        "Lunettes de protection",
        "Bouchons d'oreille",
        "Gants anti-vibration",
        "Chaussures de sécurité S3"
      ],
      "sections": [
        {
          "id": "S1",
          "name": "1. Inspection visuelle",
          "order": 1,
          "items": [
            {
              "id": "C1",
              "order": 1,
              "check": "État des fixations et supports",
              "description": "Vérifier que toutes les fixations sont serrées et non corrodées",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "high"
            },
            {
              "id": "C2",
              "order": 2,
              "check": "Fuites d'huile/gaz détectées",
              "description": "Inspecter visuellement et avec détecteur si nécessaire",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "critical"
            },
            {
              "id": "C3",
              "order": 3,
              "check": "Corrosion ou déformation visible",
              "description": "Examiner le carter et les composants externes",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "high"
            },
            {
              "id": "C4",
              "order": 4,
              "check": "Propreté du carter et filtres",
              "description": "Vérifier l'absence d'accumulation de poussière/débris",
              "type": "boolean",
              "mandatory": false,
              "criticalityLevel": "medium"
            },
            {
              "id": "C5",
              "order": 5,
              "check": "État des tuyauteries et raccords",
              "description": "Inspecter l'intégrité des connexions",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "high"
            }
          ]
        },
        {
          "id": "S2",
          "name": "2. Tests fonctionnels",
          "order": 2,
          "items": [
            {
              "id": "C6",
              "order": 6,
              "check": "Pression de service",
              "description": "Mesurer la pression de sortie",
              "type": "number",
              "unit": "bars",
              "range": {
                "min": 6,
                "max": 8,
                "optimal": 7
              },
              "mandatory": true,
              "criticalityLevel": "critical"
            },
            {
              "id": "C7",
              "order": 7,
              "check": "Température moteur",
              "description": "Mesurer après 10 min de fonctionnement",
              "type": "number",
              "unit": "°C",
              "range": {
                "min": 20,
                "max": 75,
                "optimal": 50
              },
              "mandatory": true,
              "criticalityLevel": "critical"
            },
            {
              "id": "C8",
              "order": 8,
              "check": "Niveau vibrations",
              "description": "Mesurer avec vibromètre aux 4 points de fixation",
              "type": "select",
              "options": [
                {"value": "low", "label": "Faible (< 2.8 mm/s)", "acceptable": true},
                {"value": "medium", "label": "Moyen (2.8-7.1 mm/s)", "acceptable": true},
                {"value": "high", "label": "Élevé (> 7.1 mm/s)", "acceptable": false}
              ],
              "mandatory": true,
              "criticalityLevel": "high"
            },
            {
              "id": "C9",
              "order": 9,
              "check": "Niveau sonore",
              "description": "Mesure à 1m du compresseur",
              "type": "number",
              "unit": "dB(A)",
              "range": {
                "min": 0,
                "max": 85
              },
              "mandatory": true,
              "criticalityLevel": "medium"
            },
            {
              "id": "C10",
              "order": 10,
              "check": "Débit d'air",
              "description": "Vérifier le débit nominal",
              "type": "number",
              "unit": "m³/min",
              "range": {
                "min": 15,
                "max": 18
              },
              "mandatory": true,
              "criticalityLevel": "high"
            }
          ]
        },
        {
          "id": "S3",
          "name": "3. Sécurité et conformité OCP",
          "order": 3,
          "items": [
            {
              "id": "C11",
              "order": 11,
              "check": "Système arrêt d'urgence fonctionnel",
              "description": "Tester le bouton coup de poing",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "critical"
            },
            {
              "id": "C12",
              "order": 12,
              "check": "Détecteurs H2S opérationnels (si applicable)",
              "description": "Test des capteurs de gaz",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "critical"
            },
            {
              "id": "C13",
              "order": 13,
              "check": "EPI disponibles à proximité",
              "description": "Vérifier la présence et l'état des équipements de protection",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "high"
            },
            {
              "id": "C14",
              "order": 14,
              "check": "Signalisation de sécurité conforme",
              "description": "Panneaux d'avertissement présents et lisibles",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "medium"
            },
            {
              "id": "C15",
              "order": 15,
              "check": "Procédure d'urgence affichée",
              "description": "Consignes d'intervention visible et à jour",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "high"
            }
          ]
        },
        {
          "id": "S4",
          "name": "4. Maintenance préventive",
          "order": 4,
          "items": [
            {
              "id": "C16",
              "order": 16,
              "check": "Niveau d'huile",
              "description": "Vérifier le niveau et la qualité de l'huile",
              "type": "select",
              "options": [
                {"value": "ok", "label": "OK - Niveau correct", "acceptable": true},
                {"value": "low", "label": "Bas - À compléter", "acceptable": false},
                {"value": "dirty", "label": "Sale - À vidanger", "acceptable": false}
              ],
              "mandatory": true,
              "criticalityLevel": "high"
            },
            {
              "id": "C17",
              "order": 17,
              "check": "État des filtres à air",
              "description": "Inspecter et nettoyer si nécessaire",
              "type": "select",
              "options": [
                {"value": "clean", "label": "Propre", "acceptable": true},
                {"value": "dusty", "label": "Poussiéreux - À nettoyer", "acceptable": true},
                {"value": "clogged", "label": "Colmaté - À remplacer", "acceptable": false}
              ],
              "mandatory": true,
              "criticalityLevel": "medium"
            },
            {
              "id": "C18",
              "order": 18,
              "check": "Tension des courroies",
              "description": "Vérifier la tension et l'usure",
              "type": "boolean",
              "mandatory": true,
              "criticalityLevel": "medium"
            }
          ]
        },
        {
          "id": "S5",
          "name": "5. Observations et recommandations",
          "order": 5,
          "items": [
            {
              "id": "C19",
              "order": 19,
              "check": "Notes de l'inspecteur",
              "description": "Remarques, observations particulières, recommandations",
              "type": "textarea",
              "mandatory": false,
              "criticalityLevel": "low"
            },
            {
              "id": "C20",
              "order": 20,
              "check": "Photo de l'équipement",
              "description": "Photo d'ensemble et de tout défaut détecté",
              "type": "file",
              "acceptedFormats": ["image/jpeg", "image/png"],
              "maxSize": 5242880,
              "mandatory": false,
              "criticalityLevel": "low"
            },
            {
              "id": "C21",
              "order": 21,
              "check": "Intervention nécessaire",
              "description": "Indiquer si une intervention est requise",
              "type": "select",
              "options": [
                {"value": "none", "label": "Aucune intervention", "acceptable": true},
                {"value": "minor", "label": "Intervention mineure programmée", "acceptable": true},
                {"value": "urgent", "label": "Intervention urgente requise", "acceptable": false}
              ],
              "mandatory": true,
              "criticalityLevel": "high"
            }
          ]
        }
      ],
      "scoringRules": {
        "totalItems": 21,
        "mandatoryItems": 18,
        "passingScore": 85,
        "criticalItemsRequired": true
      },
      "createdAt": "2025-01-15T10:00:00Z",
      "updatedAt": "2026-01-20T14:30:00Z",
      "createdBy": "admin@ocp.ma",
      "approvedBy": "safety.manager@ocp.ma"
    },
